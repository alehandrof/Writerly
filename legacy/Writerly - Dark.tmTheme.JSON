{
	"name": "Typewriter (Light)",
	"author": "Alex Armstrong",
	"settings": [
		/* Basic Settings */
		{
			"settings": {
				/* Basic Colors */
				"caret": "#00aeff",
				"background": "#262b2f", // from Editorial
				"background": "#292929", // Editorial-ish desaturated
				"background": "#262626", // 15%
				"foreground": "#c4c4c4", // inverted light
				/* Gutter */
				"gutter": "#3d3d3d", // 24%
				"gutterForeground": "#9e9e9e",
				/* Line Highlight */
				"lineHighlight": "#3d3d3d", // 24%
				/* Guides & Whitespace */
				"guide": "#9e9e9e66",
				"activeGuide": "#61616166",
				"stackGuide": "#9e9e9e66",
				"invisibles": "#f0f", // this does nothing
				/* Selection */
				"selection": "#00aeff22",
				"selectionBorder": "#00aeff55",
				"inactiveSelection": "#9e9e9e33",
				/* Find */
				"findHighlight": "#ffc71977",
				"findHighlightForeground": "#684f00",
				/* Brackets & Tags */
				"bracketContentsForeground": "#00aeff",
				"bracketContentsOptions": "underline",
				"bracketsForeground": "#00aeff",
				"bracketsOptions": "underline",
				"tagsForeground": "#00aeff",
				"tagsOptions": "underline", // Options: underline, stippled_underline, squiggly_underline
				/* Shadows */
				"shadow": "#9e9e9e",
				"shadowWidth": "4",
			}
		},

		/*{
			"name": "Foreground",
			"scope": "",
			"settings": {
				"foreground": "#00000099"
			}
		},*/

		/* Styles by Color */
		{
			"name": "Black",
			"scope": "string.other.link.inline",
			"settings": {
				"foreground": "#3d3d3d"
			}
		},
		{
			"name": "Gray (Light)",
			"scope": "color.gray, comment, meta.diff, meta.diff.header, text.html.markdown punctuation, markup.separator, markup.strikethrough, markup.comment",
			"settings": {
				"foreground": "#9e9e9e"
			}
		},
		{
			"name": "Gray (Dark)",
			"scope": "color.darkgray, style.bold, markup.heading, markup.bold, markup.quote markup.italic markup.bold, markup.raw, markup.quote, string.other.link.title, punctuation.definition.list.task.checkbox.incomplete, markup.all-caps",
			"settings": {
				"foreground": "#616161"
			}
		},
		{
			"name": "Yellow",
			"scope": "color.yellow, markup.changed",
			"settings": {
				"foreground": "#b58900"
			}
		},
		{
			"name": "Orange",
			"scope": "color.orange, string, variable.parameter, variable.other",
			"settings": {
				"foreground": "#cb4b16"
			}
		},
		{
			"name": "Red",
			"scope": "color.red, markup.deleted",
			"settings": {
				"foreground": "#dc322f"
			}
		},
		{
			"name": "Magenta",
			"scope": "color.magenta, keyword, storage, entity.name.tag, constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": "#d33682"
			}
		},
		{
			"name": "Violet",
			"scope": "color.purple, constant.numeric, constant.language, constant.character, constant.other, markup.underline.link.reference, markup.highlight, constant.character.escape",
			"settings": {
				"foreground": "#6c71c4"
			}
		},
		{
			"name": "Blue",
			"scope": "color.blue, storage.type, support.function, support.constant, support.type, support.class, entity.name.filename.find-in-files, markup.underline.link",
			"settings": {
				"foreground": "#268bd2"
			}
		},
		{
			"name": "Cyan",
			"scope": "color.cyan",
			"settings": {
				"foreground": "#2aa198"
			}
		},
		{
			"name": "Green",
			"scope": "color.green, entity.name.class, entity.other.inherited-class, entity.name.function, entity.other.attribute-name, markup.inserted, string.addition",
			"settings": {
				"foreground": "#738d00"
			}
		},
		/* Reversed Colors */
		{
			"name": "Reversed Gray (Light)",
			"scope": "bgcolor.gray, markup.strikethrough",
			"settings": {
				"background": "#9e9e9e33"
			}
		},
		{
			"name": "Reversed Gray (Dark)",
			"scope": "bgcolor.darkgray",
			"settings": {
				"background": "#61616133"
			}
		},
		{
			"name": "Reversed Yellow",
			"scope": "bgcolor.yellow",
			"settings": {
				"background": "#b5890033"
			}
		},
		{
			"name": "Reversed Orange",
			"scope": "bgcolor.orange",
			"settings": {
				"background": "#cb4b1633"
			}
		},
		{
			"name": "Reversed Red",
			"scope": "bgcolor.red, invalid",
			"settings": {
				"background": "#dc322f33",
			}
		},
		{
			"name": "Reversed Magenta",
			"scope": "bgcolor.magenta, invalid.deprecated",
			"settings": {
				"background": "#d3368233"
			}
		},
		{
			"name": "Reversed Violet",
			"scope": "bgcolor.violet, constant.character.hard-line-break",
			"settings": {
				"background": "#6c71c433",
			}
		},
		{
			"name": "Reversed Blue",
			"scope": "bgcolor.blue",
			"settings": {
				"background": "#268bd233"
			}
		},
		{
			"name": "Reversed Cyan",
			"scope": "bgcolor.cyan",
			"settings": {
				"background": "#2aa19833"
			}
		},
		{
			"name": "Reversed Green",
			"scope": "bgcolor.green",
			"settings": {
				"background": "#738d0033"
			}
		},

		/* Styles by Font */
		{
			"name": "Bold",
			"scope": "style.bold, markup.heading, markup.bold, markup.quote markup.italic markup.bold, markup.quote markup.bold markup.italic, markup.all-caps", /* text.html.markdown punctuation */
			"settings": {
				"fontStyle": "bold",
			}
		},
		{
			"name": "Italic",
			"scope": "style.italic, storage.type, support.type, support.class, variable.parameter, markup.italic, markup.quote",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Underline",
			"scope": "style.underline, entity.name.class, entity.other.inherited-class, markup.inserted",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Bold & Italic",
			"scope": "style.bold.italic markup.bold markup.italic, markup.italic markup.bold, markup.quote markup.bold, markup.heading markup.italic",
			"settings": {
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Italic & Underline",
			"scope": "style.italic.underline, entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic underline"
			}
		},
		{
			"name": "No Font Style (Roman)",
			"scope": "style.none, support.other.variable, markup.quote markup.italic, text.html.markdown punctuation",
			"settings": {
				"fontStyle": ""
			}
		},

		/* Additional Styles */
		// ...
	],
	"uuid": "85a626e0-e7e2-11e2-91e2-0800200c9a66"
}